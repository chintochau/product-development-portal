{
  "name": "product-development-portal",
  "version": "1.0.5",
  "description": "BluOS product development portal",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck": "tsc --noEmit",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "publish": "npm run build && electron-builder --publish always",
    "db:init": "tsx database/init-db.ts",
    "db:reset": "tsx database/reset-db.ts",
    "db:migrate:products": "tsx database/migrations/migrate-products.ts",
    "db:migrate:features": "tsx database/migrations/migrate-features.ts",
    "db:migrate:all": "npm run db:migrate:products && npm run db:migrate:features",
    "api:dev": "tsx watch api/server.ts",
    "api:build": "tsc --project api/tsconfig.json",
    "api:start": "node api/dist/server.js"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@hookform/resolvers": "^3.9.1",
