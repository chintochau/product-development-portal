    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/lodash": "^4.17.20",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.10",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "postcss": "^8.4.49",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.15",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^5.3.1"
  },
  "build": {
    "publish": [
      {
        "provider": "s3",
        "bucket": "bluosproductportal",
        "region": "us-east-2",
        "acl": null
      }
    ],
    "releaseInfo": {
      "releaseNotesFile": "resources/release-notes.md"
    }
  }
}
